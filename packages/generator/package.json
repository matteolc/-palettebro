{
  "name": "@palettebro/generator",
  "version": "2.0.4",
  "private": false,
  "sideEffects": false,
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./types": "./dist/index.d.ts"
  },
  "keywords": [
    "palette",
    "color",
    "css",
    "design"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "echo 'Not implemented'",
    "test:coverage": "echo 'Not implemented'",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "format-and-lint": "biome check .",
    "format-and-lint:fix": "biome check . --write"
  },
  "dependencies": {
    "@material/material-color-utilities": "0.3.0",
    "apca-w3": "^0.1.9",
    "culori": "^4.0.1",
    "delta-e": "^0.0.8",
    "nearest-color": "^0.4.4",
    "valibot": "^1.1.0"
  },
  "devDependencies": {
    "@types/apca-w3": "^0.1.3",
    "@types/culori": "^2.1.1",
    "@types/delta-e": "^0.0.2"
  },
  "authors": [
    {
      "name": "Matteo La Cognata",
      "email": "matteo.lacognata@nearform.com",
      "url": "https://github.com/matteolc"
    }
  ],
  "license": "MIT",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/matteolc/-palettebro"
  },
  "homepage": "https://www.palettebro.com"
}
